{
  "name": "vite-react-template",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/xinlei3166/vite-react-template"
  },
  "keywords": [
    "react",
    "vite",
    "template"
  ],
  "author": "君惜",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build",
    "build:test": "tsc && vite build --mode test",
    "build:sim": "tsc && vite build --mode sim",
    "build": "vite build --mode online",
    "serve": "vite preview",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "release": "yunque-release --skipBuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.17.0-alpha.10",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "csstype": "^3.0.10",
    "dayjs": "^1.10.7",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "raven-js": "^3.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-sortablejs": "^6.0.0",
    "react-use": "^17.3.1",
    "redux": "^4.1.2",
    "redux-logger": "^4.0.0",
    "redux-undo": "^1.0.1",
    "reselect": "^4.1.4",
